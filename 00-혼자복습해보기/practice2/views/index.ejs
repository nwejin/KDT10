  <body>
    <h1>form 실습</h1>

    <h2>GET 요청</h2>
    <form action="/getform" method="get">
      <label for="ID"> ID </label>
      <input type="text" name="ID" id="ID" />
      <br />
      <label for="PW"> PW </label>
      <input type="text" name="PW" id="PW" />
      <br />
      <br />
      <button>제출</button>
    </form>

    <h2>POST 요청</h2>
    <form action="/postForm" method="post">
      <label for="ID"> ID </label>
      <input type="text" name="ID" id="ID" />
      <br />
      <label for="PW"> PW </label>
      <input type="text" name="PW" id="PW" />
      <br />
      <br />
      <button>제출</button>
    </form>
  </body>
</html>
